{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "declaration": true,
    "baseUrl": "./src",
    "outDir": "./dist",
    "paths": {
      "@adminapp/*": [ "./*" ]
    },
    "composite": false,
    "declarationMap": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": false,
    "isolatedModules": false,
    "jsx": "preserve",
    "incremental": true,
    
  },
  
  "include": ["src/**/*",],
  "exclude": ["node_modules","dist"]
}
